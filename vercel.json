{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/next"
        }
    ],
    "env": {
        "DB_HOST": "@db_host",
        "DB_USER": "@db_user",
        "DB_PASSWORD": "@db_password",
        "DB_NAME": "@db_name",
        "JWT_SECRET": "@jwt_secret"
    }
}